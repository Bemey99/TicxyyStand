Create('.Glitch',function()
    Chat("Attempting To Glitch Target!")
    Stop()
    Reset()
    wait(1)
    repeat rs.heartbeat:wait()
        Hit(false)
        getRoot(STAND).CFrame = getRoot(TARGET).CFrame
    until KO(TARGET).Value == true 
end)

