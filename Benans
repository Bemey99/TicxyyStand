Create('Rev!',function()
    noclip = true
    repeat rs.Stepped:Wait()
        getRoot(STAND).CFrame = game.Workspace.Ignored.Shop['[Revolver] - $1379'].Head.CFrame*CFrame.new(0,-5,0)
        fireclickdetector(game.Workspace.Ignored.Shop['[Revolver] - $1379'].ClickDetector)
    until STAND.Backpack:FindFirstChild("[Revolver]")
    STAND.Backpack:FindFirstChild('[Revolver]').Parent = STAND.Character
end
end)
