Create('CW!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Joining Crew!', 'All');
    wait(1)
    game.ReplicatedStorage.MainEvent:FireServer("JoinCrew",CrewID) 
    wait(1)
    game.ReplicatedStorage.MainEvent:FireServer("JoinCrew",CrewID) 
end)

Create('UnCW!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Leaving Crew!', 'All');
    wait(.2)
    game.ReplicatedStorage.MainEvent:FireServer("LeaveCrew")
end)

Create('SumAud!',function()
    SummonAudio()
    wait(.3)
    Chat('Started Summon Audio!')
    wait(6)
    Chat('Finished Summon Audio!')
end)

