loadstring(game:HttpGet("https://raw.githubusercontent.com/ticxyy/XD/main/V4.3"))()

Create('–°–æ—Åk!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Want To See My –°–æ—Åk üòè ü§´', 'All');
end)

Create('Ban!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Perma-Ban in 20 Seconds.', 'All');
wait(20)
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Joking!', 'All');
end)

Arg('L!',function()
    local GetPlayer = gplr(Arg)
    if GetPlayer.Name == STAND.Name then
        if ChatCmds == true then
            Chat("My Position Has Been Set To Left "..CustomName.."!")
        end
        Pos = "Left"
        if Sounds == true then
            play(8655611378,true,true) 
        end
    end
end)

