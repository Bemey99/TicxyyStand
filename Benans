loadstring(game:HttpGet("https://raw.githubusercontent.com/ticxyy/XD/main/V4.3"))()

function BuySMask()
Stop()
    noclip = true
    repeat rs.Stepped:Wait()
        getRoot(STAND).CFrame = game.Workspace.Ignored.Shop['[Pitchfork] - $64'].Head.CFrame*CFrame.new(0,-5,0)
        fireclickdetector(game.Workspace.Ignored.Shop['[Pitchfork] - $64'].ClickDetector)
    until STAND.Backpack:FindFirstChild("[Mask]")
    STAND.Backpack:FindFirstChild('[Mask]').Parent = STAND.Character
    getHumanoid(STAND):UnequipTools()
end

