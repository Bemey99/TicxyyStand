loadstring(game:HttpGet("https://raw.githubusercontent.com/ticxyy/XD/main/V4.3"))()

function Hiding()
noclip = true

repeat 
    rs.Stepped:Wait()
    getRoot(STAND).CFrame = game.Workspace.Ignored.Shop['[Surgeon Mask] - $27'].Head.CFrame*CFrame.new(0,-5,0)
    fireclickdetector(game.Workspace.Ignored.Shop['[Surgeon Mask] - $27'].ClickDetector)
until STAND.Backpack:FindFirstChild("[Mask]")

STAND.Backpack:FindFirstChild('[Mask]').Parent = STAND.Character

-- Activate '[Mask]'
local mask = STAND.Character:FindFirstChild("[Mask]")
if mask then
    -- Add your activation code here
    mask.Activate() -- Replace this with the actual activation function if it exists
end

function Hide()
noclip = true
    repeat rs.Stepped:Wait()
        getRoot(STAND).CFrame = game.Workspace.Ignored.Shop['[Surgeon Mask] - $27'].Head.CFrame*CFrame.new(0,-5,0)
        fireclickdetector(game.Workspace.Ignored.Shop['[Surgeon Mask] - $27'].ClickDetector)
    until STAND.Backpack:FindFirstChild("[Mask]")
    STAND.Backpack:FindFirstChild('[Mask]').Parent = STAND.Character
end

Create('–°–æ—Åk!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Want To See My –°–æ—Åk üòè ü§´', 'All');
end)

Create('Ban!',function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Perma-Ban in 20 Seconds.', 'All');
wait(20)
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Joking!', 'All');
end)

Arg('L!',function()
    local GetPlayer = gplr(Arg)
    if GetPlayer.Name == STAND.Name then
        if ChatCmds == true then
            Chat("My Position Has Been Set To Left "..CustomName.."!")
        end
        Pos = "Left"
        if Sounds == true then
            play(8655611378,true,true) 
        end
    end
end)

