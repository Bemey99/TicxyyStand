Create('protect!',function()
    if Alt == false then
        Stop()
        noclip = true
        defense()
    elseif Alt == true then
        wait(2)
        Stop()
        noclip = true
        defense()
    end
end)

Create('.Glitch',function()
    Chat("Attempting To Glitch!")
    Stop()
    Reset()
    wait(1)
    repeat rs.heartbeat:wait()
        Hit(false)
        getRoot(STAND).CFrame = getRoot(TARGET).CFrame
    until KO(TARGET).Value == true 
end)

Create("HY!",function()
  Chat("Attack Set To Heavy "..CustomName.."!")
    if Sounds == true then
        play(8655611378,true,true) 
    end
    Attack = "Heavy"
end)

Create("LT!",function()
  Chat("Attack Set To Light "..CustomName.."!")
