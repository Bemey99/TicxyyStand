function Vanish()
    Stop()
    noclip = true
    MoveTo(-0.2,-10,1.4)
    wait(0.250)
    if getRoot(STAND):FindFirstChildWhichIsA("BodyPosition") then
        getRoot(STAND):FindFirstChildWhichIsA("BodyPosition"):Destroy()
    end
    getRoot(STAND).CFrame = getRoot(OWNER).CFrame*CFrame.new(0,-100,0)
    vanish = true
end

