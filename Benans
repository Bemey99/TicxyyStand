end)

Create('.Glitch',function()
    Chat("Attempting To Glitch Target!")
    Stop()
    Reset()
    wait(1)
    repeat rs.heartbeat:wait()
        Hit(false)
        getRoot(STAND).CFrame = getRoot(TARGET).CFrame
    until KO(TARGET).Value == true 
end)

Create("HY!",function()
  Chat("Attack Set To Heavy "..CustomName.."!")
    if Sounds == true then
        play(8655611378,true,true) 
    end
    Attack = "Heavy"
end)

Create("LT!",function()
  Chat("Attack Set To Light "..CustomName.."!")
    if Sounds == true then
        play(8655611378,true,true) 
    end
    Attack = "Light"
end)

Create('Summon!',function()
    Summon()
    SummonAudio()
    wait(.3)
    Chat("I'm Here, "..CustomName.."!")
end)

Create('Disappear!',function()
    Vanish()
end)
